{
  "name": "webhook-saas",
  "version": "0.0.1",
  "description": "A simple library to manage and create webhooks for your application",
  "main": "dist/webook-sass.cjs.js",
  "module": "dist/webook-sass.esm.js",
  "types": "dist/webook-sass.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "node-fetch": "^3.2.3",
